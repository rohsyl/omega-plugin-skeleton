{
    "name": "rohsyl/omega-plugin-skeleton",
    "description": "OmegaCMS Plugin Skeleton",
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "rohsyl",
            "email": "syzin12@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.0.2",
        "illuminate/contracts": "^9.0",
        "laravelcollective/html": "^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0",
        "nunomaduro/collision": "^6.0",
        "phpunit/phpunit": "^9"
    },
    "autoload": {
        "psr-4": {
            "rohsyl\\OmegaPlugin\\[PluginName]\\": "src",
            "rohsyl\\OmegaPlugin\\[PluginName]\\\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "rohsyl\\OmegaPlugin\\[PluginName]\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "rohsyl\\OmegaPlugin\\[PluginName]\\ServiceProvider"
            ]
        }
    }
}
